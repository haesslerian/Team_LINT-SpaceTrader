<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spacetrader">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".views.TravelScreenActivity"></activity>
        <activity android:name=".views.BuyGoodsActivity" />
        <activity android:name=".views.PlanetViewScreenActivity" />
        <activity android:name=".views.CreateCharacterSummaryActivity" />
        <activity android:name=".views.CreateCharacterBasicInfoActivity" />
        <activity android:name=".views.StartScreenActivity" />
        <activity android:name=".views.TitleScreenActivity" />
        <activity android:name=".views.SetSkillsActivity" />
        <activity android:name=".views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>